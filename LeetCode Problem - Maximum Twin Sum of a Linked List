class Solution {
    public int pairSum(ListNode head) {
        ListNode temp=head;
        int c=0;
        while(temp!=null){
          temp=temp.next;
          c++;
        }
        int[] a=new int[c];
        temp=head;
        for(int i=0;i<c;i++){
            a[i]=temp.val;
            temp=temp.next;
        }
        int max=0;
         for(int i=0;i<c/2;i++){
           if(a[i]+a[c-1-i]>max){
              max=a[i]+a[c-1-i];
           }
        }
        return max;
    }
}
